{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryg3"
		},
		"Entrada_Extraccion_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Entrada_Extraccion'"
		},
		"base_de_datos_Extraccion_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'base_de_datos_Extraccion'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryg3_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Entrada_Extraccion_connectionString": {
						"value": "[parameters('Entrada_Extraccion_connectionString')]"
					},
					"base_de_datos_Extraccion_connectionString": {
						"value": "[parameters('base_de_datos_Extraccion_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}